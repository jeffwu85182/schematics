{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsKtbpFeature",
  "title": "Ktbp Feature Module Options Schema",
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "default": "src"
    },
    "sourceDir": {
      "type": "string",
      "default": "src",
      "alias": "sd"
    },
    "name": {
      "type": "string"
    },
    "flat": {
      "type": "boolean",
      "default": false,
      "description": "Flag to indicate if a dir is created."
    },
    "module": {
      "type": "string",
      "description": "Specifies the declaring module.",
      "aliases": ["m"]
    },
    "spec": {
      "type": "boolean",
      "default": true,
      "description": "Specifies if a spec file is generated."
    },
    "routing": {
      "type": "boolean",
      "default": false
    }
  },
  "required": [
    "name"
  ]
}
